{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": ["es6", "dom"],
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react",
    "incremental": true
  },
  "exclude": [
    "node_modules",
    "build",
    "scripts",
  ],
  "include": ["**/*.ts", "**/*.tsx", "src/constants/sideNavbarItemsjs", "src/components/SideNavBar.tsx", "src/components/NavBarMenus.tsx"],
}
